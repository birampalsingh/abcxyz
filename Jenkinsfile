pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        echo 'my first pipeline'
      }
    }
    stage('Test') {
      steps {
        echo 'this is teting'
      }
    }
    stage('deploye') {
      steps {
        echo 'this is deploye'
      }
    }
    stage('Done') {
      steps {
        echo 'this is complete'
      }
    }
  }
}